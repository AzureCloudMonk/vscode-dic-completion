{
    "name": "dictionary-completion",
    "displayName": "Dictionary Completion",
    "description": "Word Completion",
    "icon": "images/dictionary.png",
    "version": "0.5.0",
    "publisher": "yzhang",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/neilsustc/vscode-dic-completion"
    },
    "activationEvents": [
        "onLanguage:markdown",
        "onLanguage:latex",
        "onLanguage:html"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "completion.openUserDict",
                "title": "Completion: Open User Dictionary"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}